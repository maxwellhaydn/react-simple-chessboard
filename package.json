{
  "name": "react-simple-chessboard",
  "version": "1.0.0",
  "description": "A minimal React chessboard component based on chessboard.js",
  "main": "src/index.js",
  "scripts": {
    "test": "webpack --config webpack.chessboardjs.config.js --mode development && jest",
    "test:watch": "npm test -- --watch",
    "build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxwellhaydn/react-simple-chessboard.git"
  },
  "keywords": [
    "react",
    "chess",
    "chessboard",
    "chessboard.js"
  ],
  "author": "Max Carey",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/maxwellhaydn/react-simple-chessboard/issues"
  },
  "homepage": "https://github.com/maxwellhaydn/react-simple-chessboard#readme",
  "peerDependencies": {
    "react": "^16.8.0",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-jest-mocks": "^1.2.4",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^23.16.0",
    "eslint-plugin-react": "^7.20.0",
    "exports-loader": "^1.0.1",
    "jest-cli": "^26.0.1",
    "jquery": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@chrisoakman/chessboardjs": "^1.0.0",
    "prop-types": "^15.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^@chrisoakman/chessboardjs$": "<rootDir>/dist/chessboard.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
